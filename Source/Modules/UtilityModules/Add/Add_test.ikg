<group name="Test">
	<module class = "Constant" name = "Constant1" data="1, 1; 3, 4" />
	<module class = "Constant" name = "Constant2" data="8, 9; 10, 11" />
	<module class = "Add" name = "Add" />

	<_module class = "Print" name = "Print" />
	<module class = "Logger" name = "Logger" />	

	<connection  source = "Constant1.OUTPUT"  	target = "Add.INPUT1"  />
	<connection  source = "Constant2.OUTPUT"  	target = "Add.INPUT2" />		<!-- NO recurrent connection-->

	<_connection  source = "Add.OUTPUT"  target = "Print.INPUT" />

	<view name="View">

		<table
			class = "table"
			height = "300"
			title = "A"
			width = "300"
			source = "Constant1.OUTPUT"
			y = "20"
			x = "40"
			decimals = "4"
			colorize = "true"
			show_title = "true"
			show_frame = "true"
		/>

		<table
			class = "table"
			height = "300"
			title = "B"
			width = "300"
			source = "Constant2.OUTPUT"
			y = "20"
			x = "360"
			decimals = "4"
			colorize = "true"
			show_title = "true"
			show_frame = "true"
		/>

		<table
			class = "table"
			height = "300"
			title = "Sum"
			width = "300"
			source = "Add.OUTPUT"
			y = "360"
			x = "200"
			decimals = "4"
			colorize = "true"
			show_title = "true"
			show_frame = "true"
		/>

	</view>

</group>

