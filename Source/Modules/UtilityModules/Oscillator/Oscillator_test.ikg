<group name="Test">
	<module class = "Oscillator" name = "Oscillator" type="sin" frequency="1; 0.5; 2" />

	<module class = "Print" name = "Print" />
	<module class = "Logger" name = "Logger"/>
	<connection  source = "Oscillator.OUTPUT"  	target = "Print.INPUT" />

	<view name="View">
		<widget
			class = "table"
			height = "300"
			title = "Osc frequency"
			width = "300"
			source = "Oscillator.frequency"
			y = "20"
			x = "40"
			decimals = "4"
			colorize = "true"
			show_title = "true"
			show_frame = "true"
		/>

		<widget
			class = "bar-graph"
			height = "300"
			title = "Osc Out"
			width = "300"
			source = "Oscillator.OUTPUT"
			y = "20"
			x = "450"
			decimals = "1"
			min = "-1"
			colorize = "true"
			show_title = "true"
			show_frame = "true"
		/>

		<widget
			class = "plot"
			height = "300"
			title = "Osc Out"
			width = "600"
			source = "Oscillator.OUTPUT"
			y = "440"
			x = "40"
			decimals = "1"
			min = "-1"
			colorize = "true"
			show_title = "true"
			show_frame = "true"
		/>

	</view>
</group>



